.tab-box {
  width: 100%;
  color: rgba(0, 0, 0, 0.8);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 90rpx;
  background: #fff;
  font-size: 28rpx;
  box-shadow: 0 1px 5px rgba(0, 0, 0, 0.06);
  position: relative;
  z-index: 10;
  overflow: hidden;
}
.tab-box .active {
  color: #e54d42;
}
.tab-box .horizontal {
  width: 100%;
}
.tab-box .horizontal .item {
  display: inline-block;
  text-align: center;
  padding: 0 30rpx;
  height: 86rpx;
  line-height: 90rpx;
}
.tab-box .horizontal .underline {
  height: 4rpx;
  background-color: #e54d42;
  border-radius: 3px;
  -webkit-transition: 0.5s;
  transition: 0.5s;
}

